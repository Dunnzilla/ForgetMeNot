<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	<EditTextPreference android:title="Some Value"
					    android:key="someval"
					    android:summary="Please provide some value"
    ></EditTextPreference>
	<EditTextPreference android:title="Some Other Value"
					    android:key="someval2"
					    android:summary="Please provide some other value"
    ></EditTextPreference>
     <RingtonePreference android:showDefault="true"
         android:key="Audio" android:title="Reminder Tone"
         android:ringtoneType="notification">
      </RingtonePreference>
      
 	<ListPreference
  		android:title="Notify frequency"
  		android:summary="How often to generate notifications for due Reminders"
  		android:key="pref_notify_delay"
  		android:entries="@array/pref_notify_delay_options"
		android:entryValues="@array/pref_notify_delay_values"
	/>      
</PreferenceScreen>